{
    "vits": {
        "segment_size": 32,
        "generator": {
            "decoder": {
                "sample_rate": 48000,
                "frame_size": 480,
                "n_fft": 1920,
                "speaker_embedding_dim": 256,
                "content_channels": 192,
                "pe_internal_channels": 256,
                "pe_num_layers": 4,
                "source_internal_channels": 256,
                "source_num_layers": 4,
                "num_harmonics": 30,
                "filter_channels": [256, 128, 64, 32, 16],
                "filter_factors": [
                    5,
                    4,
                    4,
                    3,
                    2
                ],
                "filter_resblock_type": "1",
                "filter_down_dilations": [
                    [1, 2],
                    [4, 8]
                ],
                "filter_down_interpolation": "conv",
                "filter_up_dilations": [
                    [1, 3, 5],
                    [1, 3, 5],
                    [1, 3, 5]
                ],
                "filter_up_kernel_sizes": [
                    3,
                    7,
                    11
                ],
                "filter_up_interpolation": "conv"
            },
            "posterior_encoder": {
                "n_fft": 1920,
                "frame_size": 480,
                "internal_channels": 192,
                "speaker_embedding_dim": 256,
                "content_channels": 192,
                "kernel_size": 5,
                "dilation": 1,
                "num_layers": 16
            },
            "speaker_embedding": {
                "num_speakers": 8192,
                "embedding_dim": 256
            },
            "prior_encoder": {
                "flow": {
                    "content_channels": 192,
                    "internal_channels": 192,
                    "speaker_embedding_dim": 256,
                    "kernel_size": 5,
                    "dilation": 1,
                    "num_layers": 4,
                    "num_flows": 4
                },
                "text_encoder": {
                    "num_phonemes": 512,
                    "num_languages": 256,
                    "lm_dim": 768,
                    "internal_channels": 256,
                    "speaker_embedding_dim": 256,
                    "content_channels": 192,
                    "n_heads": 4,
                    "num_layers": 4,
                    "window_size": 4
                },
                "stochastic_duration_predictor": {
                    "in_channels": 192,
                    "filter_channels": 256,
                    "kernel_size": 5,
                    "p_dropout": 0.0,
                    "speaker_embedding_dim": 256
                },
                "duration_predictor": {
                    "content_channels": 192,
                    "internal_channels": 256,
                    "speaker_embedding_dim": 256,
                    "kernel_size": 7,
                    "num_layers": 4
                }
            }
        },
        "discriminator": {
            "mrd": {
                "resolutions": [
                    128,
                    256,
                    512
                ],
                "channels": 32,
                "max_channels": 256,
                "num_layers": 4
            },
            "mpd": {
                "periods": [
                    1,
                    2,
                    3,
                    5,
                    7,
                    11,
                    17,
                    23,
                    31
                ],
                "channels": 32,
                "channels_mul": 4,
                "max_channels": 1024,
                "num_layers": 4
            }
        },
        "duration_discriminator": {
            "content_channels": 192,
            "speaker_embedding_dim": 256,
            "num_layers": 3
        },
        "optimizer": {
            "lr": 1e-4,
            "betas": [
                0.8,
                0.99
            ]
        }
    },
    "language_model": {
        "type": "rinna_roberta",
        "options": {
            "hf_repo": "rinna/japanese-roberta-base",
            "layer": 12
        }
    },
    "preprocess": {
        "sample_rate": 48000,
        "max_waveform_length": 480000,
        "pitch_estimation": "fcpe",
        "max_phonemes": 100,
        "lm_max_tokens": 30,
        "frame_size": 480,
        "cache": "dataset_cache"
    },
    "train": {
        "save": {
            "models_dir": "models",
            "interval": 400
        },
        "data_module": {
            "cache_dir": "dataset_cache",
            "metadata": "models/metadata.json",
            "batch_size": 56,
            "num_workers": 63
        },
        "trainer": {
            "devices": "auto",
            "max_epochs": 1000000,
            "precision": null
        }
    },
    "infer": {
        "n_fft": 1920,
        "frame_size": 480,
        "sample_rate": 48000,
        "max_lm_tokens": 50,
        "max_phonemes": 500,
        "max_frames": 2000,
        "device": "cuda"
    }
}