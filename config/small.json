{
	"generator": {
		"decoder": {
			"sample_rate": 48000,
			"frame_size": 960,
			"n_fft": 3840,
			"content_channels": 192,
			"pe_internal_channels": 256,
			"pe_num_layers": 4,
			"source_internal_channels": 256,
			"source_num_layers": 3,
			"num_harmonics": 14,
			"filter_channels": [384, 192, 96, 48, 24],
			"filter_factors": [3, 4, 4, 4, 5],
			"filter_resblock_type": "3",
			"filter_down_dilations": [[1, 2, 4]],
			"filter_up_dilations": [[1, 3, 9, 27]],
			"filter_up_kernel_sizes": [3]
		},
		
		"posterior_encoder": {
			"n_fft" : 3840,
			"internal_channels": 192,
			"speaker_embedding_dim": 256,
			"content_channels": 192,
			"kernel_size": 5,
			"dilation": 1,
			"num_layers": 16
		},

		"flow": {
			"content_channels": 192,
			"internal_channels": 192,
			"speaker_embedding_dim": 256,
			"kernel_size": 5,
			"dilation": 1,
			"num_layers": 4,
			"num_flows": 4
		},

		"speaker_embedding": {
			"num_speakers": 8192,
			"embedding_dim": 256
		},

		"text_encoder": {
			"num_phonemes": 512,
			"num_languages": 256,
			"lm_dim": 768,
			"internal_channels": 256,
			"content_channels": 192,
			"n_heads": 4,
			"num_layers": 4,
			"window_size": 4
		},

		"stochastic_duration_predictor": {
			"in_channels": 192,
			"filter_channels": 256,
			"kernel_size": 5,
			"p_dropout": 0.0,
			"speaker_embedding_dim": 256
		},

		"duration_predictor": {
			"content_channels": 192,
			"internal_channels": 256,
			"speaker_embedding_dim": 256,
			"kernel_size": 7,
			"num_layers": 4
		},

		"audio_encoder": {
			"n_fft": 3840,
			"internal_channels": 192,
			"content_channels": 192,
			"kernel_size": 7,
			"num_layers": 4
		}
	},

	"discriminator": {
		"resolutions": [128, 256, 512],
		"periods": [1, 2, 3, 5, 7, 11],
		"mpd_num_layers": 4,
		"mrd_num_layers": 4,
		"mpd_channels": 32,
		"mrd_channels": 32,
		"mpd_max_channels": 256,
		"mrd_max_channels": 256
	},

	"duration_discriminator": {
		"content_channels": 192,
		"speaker_embedding_dim": 256,
		"internal_channels": 256,
		"kernel_size": 7,
		"num_layers": 4
	},

	"preprocess": {
		"sample_rate": 48000,
		"max_waveform_length": 480000,
		"pitch_estimation": "fcpe",
		"max_phonemes": 50,
		"lm_max_tokens": 30,
		"frame_size": 960,
		"cache": "dataset_cache"
	},

	"language_model": {
		"type": "rinna_roberta",
		"options": {
			"hf_repo": "rinna/japanese-roberta-base",
			"layer": 12
		}
	},

	"train": {
		"cache": "dataset_cache",
		"device": "cuda",
		"epoch": 100,
		"batch_size": 2,
		"gradient_accumulation": 1,
		"lr": 1e-4,
		"interval": 300
	}
}
